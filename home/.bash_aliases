# ---------------------------------------------------
# Custom aliases
# ---------------------------------------------------
alias addsite="sudo addvhost"
alias removesite="sudo removehost"
alias usephp7.0="sudo use-php 7.0"
alias usephp7.1="sudo use-php 7.1"
alias usephp7.2="sudo use-php 7.2"
alias gruntbase="git clone git@github.com:magnus-eriksson/gruntbase.git src && cd src && sudo rm -R .git && npm install"



